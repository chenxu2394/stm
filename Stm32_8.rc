        MENUITEM "IO &Controls",                IOCTRL
        MENUITEM "&Input...",                   INPUT
        MENUITEM "&Output...",                  OUTPUT
        MENUITEM "Osci&lloscope",               OSCILLOSCOPE
        MENUITEM "&Mass Spec",                  MASSPEC, GRAYED
        MENUITEM "&Heater",                     HEATER, GRAYED
    END
    POPUP "&Control"
    BEGIN
        MENUITEM "&Tip Approach...",            TIP
        MENUITEM "&Coarse Motion...",           COARSE, GRAYED
        MENUITEM "&Fine Motion...",             FINE, GRAYED
    END
    POPUP "&Scanning"
    BEGIN
        MENUITEM "&Scan...",                    SCAN
        MENUITEM "&Deposition",                 MENU_SCANNING_DEPOSITION
        MENUITEM "Data &Manipulation",          MENU_DATA_MANIPULATION
